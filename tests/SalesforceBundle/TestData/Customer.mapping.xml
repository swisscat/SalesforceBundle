<?xml version="1.0" encoding="UTF-8"?>

<salesforce-mapping xmlns="https://github.com/swisscat/SalesforceBundle"
                    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                    xsi:schemaLocation="https://github.com/swisscat/SalesforceBundle
                                        https://raw.githubusercontent.com/swisscat/SalesforceBundle/master/src/Resources/config/salesforce/schema/mapping-1.0.xsd">
    <entity class="Swisscat\SalesforceBundle\Test\TestData\Customer" object="Contact">
        <id name="id" externalId="None" />
        <salesforce-id type="mappingTable" />
        <property name="firstName" field="FirstName" />
        <property name="lastName" field="LastName" />
        <property name="email" field="Email" />
    </entity>
</salesforce-mapping>
<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                                      http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="Swisscat\SalesforceBundle\Entity\SalesforceMapping" table="salesforce_bundle_mapping">
        <id name="id" column="id" type="integer">
            <generator strategy="AUTO" />
        </id>
        <field name="salesforceId" column="salesforce_id" type="string" nullable="false" />
        <field name="entityId" column="entity_id" type="string" nullable="false" />
        <field name="entityType" column="entity_type" type="string" nullable="false" />
    </entity>
</doctrine-mapping>

{
  "name": "swisscat/salesforce-bundle",
  "type": "symfony-bundle",
  "description": "A Salesforce bundle for Sylius framework",
  "keywords": [ "salesforce", "sylius" ],
  "homepage": "https://github.com/swisscat/SalesforceBundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Etienne Favre",
      "email": "swisscatman@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "doctrine/orm": "*",
    "guzzlehttp/guzzle": "6.3.*",
    "phpforce/soap-client": "*",
    "php-amqplib/rabbitmq-bundle": "1.*",
    "symfony/http-kernel": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": { "Swisscat\\SalesforceBundle\\": "src/" }
  },
  "autoload-dev": {
    "psr-4": { "Swisscat\\SalesforceBundle\\Test\\": "tests/SalesforceBundle/" }
  },
  "minimum-stability": "dev"
}